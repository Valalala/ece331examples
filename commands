sqlite3 datadb
.schema
select * from data;
select * from data where pressure>=1000;
select * from data where pressure>=1000 and temp<=70;
select * from data where rowid=1;
.quit

sqlite3 newdb
create table mike (food text, air real, cash integer);
.schema
.tables
insert into mike values ("carrots",3.1415,-20);
select * from mike;
select cash from mike;
select sum(cash) from mike;
insert into mike values ("spinnach",3.1415,50);
select * from mike;
select sum(cash) from mike where cash<0;
select sum(cash) from mike;
.quit


